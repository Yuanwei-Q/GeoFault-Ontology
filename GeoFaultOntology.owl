    <!-- http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#Cataclasite -->

    <owl:Class rdf:about="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#Cataclasite">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#fault_rock"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000086"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#cohesive"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000086"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#Large_Clasts_proportion"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#has_quality_value"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.3</xsd:maxExclusive>
                                            </rdf:Description>
                                        </owl:withRestrictions>
                                    </rdfs:Datatype>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 xml:lang="en">Cataclasite: a cohesive Brittle Fault Rock containing maximum in volume 30% “large” clasts 
Superclass: Brittle Fault Rock (Def.13)</obo:IAO_0000115>
        <rdfs:comment xml:lang="en">Reference: Woodcock and Mort (2008).</rdfs:comment>
        <rdfs:label xml:lang="en">cataclasite</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#Composite_Shape -->

    <owl:Class rdf:about="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#Composite_Shape">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#Fault_Surface_Shape"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#has_quality_value"/>
                        <owl:allValuesFrom>
                            <rdfs:Datatype>
                                <owl:oneOf>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>composite</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </owl:oneOf>
                            </rdfs:Datatype>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en">composite shape</rdfs:label>
