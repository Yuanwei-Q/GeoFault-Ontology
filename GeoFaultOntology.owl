    <!-- http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#low-angle_fault -->

    <owl:Class rdf:about="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#low-angle_fault">
