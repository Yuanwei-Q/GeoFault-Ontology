        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000058"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#fault_dip_angle"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#has_quality_value"/>
                                <owl:someValuesFrom>
                                    <rdfs:Datatype>
                                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                                        <owl:withRestrictions rdf:parseType="Collection">
                                            <rdf:Description>
                                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#int">30</xsd:maxInclusive>
