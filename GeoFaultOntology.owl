                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/quy/ontologies/2022/8/untitled-ontology-33#fault_wall_volume"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/geologicalspatialrelationsontology#UFRGS:GeologicalSpatialRelationsOntology_externally_connected_with"/>
